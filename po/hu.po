#: ../build/src/preferences_gen.h:2608 ../build/src/preferences_gen.h:2711
#: ../build/src/preferences_gen.h:2798
msgctxt "preferences"
msgid "TRUE"
msgstr "IGEN"

#: ../build/src/preferences_gen.h:1679
msgid "always ask the user before any image is removed from DB."
msgstr ""
"mindig kérdezze meg a felhasználót, mielőtt eltávolítaná a képet az "
"adatbázisból."

#: ../build/src/preferences_gen.h:1687
msgid "ask before erasing images from disk / discarding history stack"
msgstr ""
"kérdezzen, mielőtt törli a képet a lemezről / elveti az előzményvermetet"

#: ../build/src/preferences_gen.h:1699
msgid ""
"always ask the user before any image file is deleted / history stack is "
"discarded."
msgstr ""
"mindig kérdezze meg a felhasználót, mielőtt törölné a képet a lemezről / "
"elveti az előzményvermet."

#: ../build/src/preferences_gen.h:1707
msgid "send files to trash when erasing images"
msgstr "törlésnél kép áthelyezése a kukába"

#: ../build/src/preferences_gen.h:1719
msgid ""
"send files to trash instead of permanently deleting files on system that "
"supports it"
msgstr ""
"a végleges törlés helyett a fájlt a kukába helyezi át, amennyiben a rendszer "
"támogatja azt"

#: ../build/src/preferences_gen.h:1727
msgid "ask before moving images from film roll folder"
msgstr "kérdezzen, mielőtt képet helyezne át egy filmszalagról"

#: ../build/src/preferences_gen.h:1739
msgid "always ask the user before any image file is moved."
msgstr "mindig kérdezze meg a felhasználót, mielőtt egy képfájlt mozgatna."

#: ../build/src/preferences_gen.h:1747
msgid "ask before copying images to new film roll folder"
msgstr "kérdezzen, mielőtt képeket másolna egy új filmszalag könyvtárba"

#: ../build/src/preferences_gen.h:1759
msgid "always ask the user before any image file is copied."
msgstr "mindig kérdezze meg a felhasználót, mielőtt egy képfájlt másolna."

#: ../build/src/preferences_gen.h:1767
msgid "ask before removing empty folders"
msgstr "kérdezzen, mielőtt eltávolítja az üres könyvtárakat"

#: ../build/src/preferences_gen.h:1779
msgid ""
"always ask the user before removing any empty folder. this can happen after "
"moving or deleting images."
msgstr ""
"mindig kérdezze meg a felhasználót, mielőtt törölne egy üres könyvtárat. ez "
"képek törlése vagy mozgatása után történhet meg."

#: ../build/src/preferences_gen.h:1787
msgid "number of folder levels to show in lists"
msgstr "listában megjelenítendő könyvtártármélység"

#: ../build/src/preferences_gen.h:1806
msgid ""
"the number of folder levels to show in film roll names, starting from the "
"right"
msgstr ""
"azon könyvtárszintek száma (jobbról számolva), amik meg kell jelenjenek a "
"filmszalag címében"

#: ../build/src/preferences_gen.h:1814
msgid "ignore JPEG images when importing film rolls"
msgstr "JPEG fájlok figyelmen kívül hagyása filmszalag importálásakor"

#: ../build/src/preferences_gen.h:1826
msgid ""
"when having raw+JPEG images together in one directory it makes no sense to "
"import both. with this flag one can ignore all JPEGs found."
msgstr ""
"ha egy könyvtárban raw+JPEG fájlok együtt találhatóak, nincs értelme "
"mindkettőt importálni. ezzel a beállítással a JPEG-ek figyelmen kívül "
"hagyhatóak."

#: ../build/src/preferences_gen.h:1834
msgid "recursive directory traversal when importing filmrolls"
msgstr "alkönyvtárak bejárása filmszalag importálásánál"

#: ../build/src/preferences_gen.h:1853
msgid "creator to be applied when importing"
msgstr "importálásnál beállítandó alkotó"

#: ../build/src/preferences_gen.h:1876
msgid "publisher to be applied when importing"
msgstr "importálásnál beállítandó kiadó"

#: ../build/src/preferences_gen.h:1899
msgid "rights to be applied when importing"
msgstr "importálásnál beállítandó szerzői jog"

#: ../build/src/preferences_gen.h:1922
msgid "comma separated tags to be applied when importing"
msgstr "importálásnál beállítandó címkék, vesszővel elválasztva"

#: ../build/src/preferences_gen.h:1945
msgid "initial import rating"
msgstr "importáláskori kezdeti értékelés"

#: ../build/src/preferences_gen.h:1966
msgid "initial star rating for all images when importing a filmroll"
msgstr "kezdeti csillag értékelés minden importált képre"

#: ../build/src/preferences_gen.h:1974
msgid "enable filmstrip"
msgstr "filmszalag engedélyezése"

#: ../build/src/preferences_gen.h:1986
msgid "enable the filmstrip in darkroom, tethering and map modes."
msgstr "filmszalag engedélyezése sötétkamra, tethering és térkép módokban."

#: ../build/src/preferences_gen.h:1994
msgid "high quality thumb processing from size"
msgstr "jó minőségű bélyegkép feldolgozás ettől a mérettől:"

#: ../build/src/preferences_gen.h:2005 ../build/src/preferences_gen.h:2738
msgctxt "preferences"
msgid "always"
msgstr "mindig"

#: ../build/src/preferences_gen.h:2010
msgctxt "preferences"
msgid "small"
msgstr "kicsi"

#: ../build/src/preferences_gen.h:2015
msgctxt "preferences"
msgid "VGA"
msgstr "VGA"

#: ../build/src/preferences_gen.h:2020 ../build/src/preferences_gen.h:2060
msgctxt "preferences"
msgid "720p"
msgstr "720p"

#: ../build/src/preferences_gen.h:2025
msgctxt "preferences"
msgid "1080p"
msgstr "1080p"

#: ../build/src/preferences_gen.h:2030
msgctxt "preferences"
msgid "WQXGA"
msgstr "WQXGA"

#: ../build/src/preferences_gen.h:2035
msgctxt "preferences"
msgid "4K"
msgstr "4K"

#: ../build/src/preferences_gen.h:2040
msgctxt "preferences"
msgid "5K"
msgstr "5K"

#: ../build/src/preferences_gen.h:2045 ../build/src/preferences_gen.h:2733
msgctxt "preferences"
msgid "never"
msgstr "soha"

#: ../build/src/preferences_gen.h:2063
msgid ""
"if the thumbnail size is greater than this value, it will be processed using "
"the full quality rendering path (better but slower)."
msgstr ""
"ha a bélyegkép mérete nagyobb mint ez az érték, a teljes minőségű "
"renderelési útvonallal lesz feldogozva (jobb, de lassabb)."

#: ../build/src/preferences_gen.h:2071
msgid "pen pressure control for brush masks"
msgstr "ceruza nyomás vezérlés ecsetmaszknál"

#: ../build/src/preferences_gen.h:2082 ../build/src/preferences_gen.h:2122
msgctxt "preferences"
msgid "off"
msgstr "ki"

#: ../build/src/preferences_gen.h:2087
msgctxt "preferences"
msgid "hardness (relative)"
msgstr "keménység (relatív)"

#: ../build/src/preferences_gen.h:2092
msgctxt "preferences"
msgid "hardness (absolute)"
msgstr "keménység (abszolút)"

#: ../build/src/preferences_gen.h:2097
msgctxt "preferences"
msgid "opacity (relative)"
msgstr "átlátszóság (relatív)"

#: ../build/src/preferences_gen.h:2102
msgctxt "preferences"
msgid "opacity (absolute)"
msgstr "átlátszóság (abszolút)"

#: ../build/src/preferences_gen.h:2107
msgctxt "preferences"
msgid "brush size (relative)"
msgstr "ecsetméret (relatív)"

#: ../build/src/preferences_gen.h:2125
msgid ""
"off - pressure reading ignored, hardness/opacity/brush size - pressure "
"reading controls specified attribute, absolute/relative - pressure reading "
"is taken directly as attribute value or multiplied with pre-defined setting."
msgstr ""
"ki - nyomásérték figyelmen kívül hagyva; keménység/átlátszóság/ecsetméret - "
"a nyomásérték az adott tulajdonságot befolyásolja; abszolút/relatív - a "
"nyomásérték közvetlenül kerül a tulajdonságértékbe, vagy egy előre megadott "
"konstanssal szorozva"

#: ../build/src/preferences_gen.h:2133
msgid "smoothing of brush strokes"
msgstr "ecsetvonások simítása"

#: ../build/src/preferences_gen.h:2144
msgctxt "preferences"
msgid "low"
msgstr "alacsony"

#: ../build/src/preferences_gen.h:2149 ../build/src/preferences_gen.h:2169
msgctxt "preferences"
msgid "medium"
msgstr "közepes"

#: ../build/src/preferences_gen.h:2154
msgctxt "preferences"
msgid "high"
msgstr "magas"

#: ../build/src/preferences_gen.h:2172
msgid ""
"sets level for smoothing of brush strokes. stronger smoothing leads to less "
"nodes and easier editing but with lower control of accuracy."
msgstr ""
"ecsetvonások simításának mértéke. erősebb simítás kevesebb pontot "
"eredményez, könnyebb szerkeszteni, de a pontosság kevésbé kontrollálható"

#: ../build/src/preferences_gen.h:2180
msgid "display of individual color channels"
msgstr "egyedi színcsatornák megjelenítése"

#: ../build/src/preferences_gen.h:2191 ../build/src/preferences_gen.h:2211
msgctxt "preferences"
msgid "false color"
msgstr "hamis színes"

#: ../build/src/preferences_gen.h:2196
msgctxt "preferences"
msgid "grey scale"
msgstr "szürkeárnyalatos"

#: ../build/src/preferences_gen.h:2214
msgid ""
"defines how color channels are displayed when activated in the parametric "
"masks feature."
msgstr ""
"meghatározza, hogy a színcsatornák hogyan jelenjenek meg, amikor aktívak a "
"paraméteres maszkon"

#: ../build/src/preferences_gen.h:2222
msgid "ask before deleting a tag"
msgstr "címke törlése előtt kérdezzen"

#: ../build/src/preferences_gen.h:2241
msgid "only draw images on map that are currently collected and filtered"
msgstr ""
"csak az aktuális gyűjtemény és szűrőbeállítás képeinek térképre helyezése"

#: ../build/src/preferences_gen.h:2253
msgid ""
"use the current filter settings to select the geotagged images drawn on the "
"map, i.e. limit the images drawn to the current filmstrip. this limits the "
"number of images drawn and thus reduces the time needed to draw the images."
msgstr ""
"az aktuális szűrési feltételeknek megfelelő geotaggelt képek megjelenítése a "
"térképen, azaz a pillanatnyi filmszalagra limitálja a kirajzolt képeket. ez "
"a képek korlátozott számának köszönhetően lecsökkenti a kirajzolási időt"

#: ../build/src/preferences_gen.h:2261
msgid "maximum number of images drawn on map"
msgstr "a térképre kirajzolt képek maximális száma"

#: ../build/src/preferences_gen.h:2280
msgid ""
"the maximum number of geotagged images drawn on the map. increasing this "
"number can slow drawing of the map down."
msgstr "a térképre kirakott geotaggelt képek maximális száma"

#: ../build/src/preferences_gen.h:2288
msgid "pretty print the image location"
msgstr "kép készítési helyének formázása"

#: ../build/src/preferences_gen.h:2300
msgid ""
"show a more readable representation of the location in the image information "
"module"
msgstr ""
"a képinformációs dobozban a koordináták olvashatóbb (fok-perc-másodperc) "
"formában jelennek meg"

#: ../build/src/preferences_gen.h:2308
msgid "overlay txt sidecar over zoomed images"
msgstr "txt kísérőfájl rávetítése a zoomolt képekre"

#: ../build/src/preferences_gen.h:2320
msgid ""
"when there is a txt file next to an image it can be shown as an overlay over "
"zoomed images on the lighttable. the txt file either has to be there at "
"import time or the crawler has to be enabled"
msgstr ""
"ha a kép mellett egy .txt fájl található, a tartalma megjeleníthető az "
"átvilágítóban a kinagyított képek fölött. a txt fájl az importáláskor a kép "
"mellett kell legyen, vagy engedélyezni kell a crawlert"

#: ../build/src/preferences_gen.h:2328
msgid "expand a single lighttable module at a time"
msgstr "egyszerre csak egy átvilágító modul lehet nyitva"

#: ../build/src/preferences_gen.h:2340
msgid "this option toggles the behavior of shift clicking in lighttable mode"
msgstr ""
"ez a beállítás a shift+klikk viselkedését változtatja meg átvilágító módban"

#: ../build/src/preferences_gen.h:2348
msgid "expand a single darkroom module at a time"
msgstr "egyszerre csak egy sötétkamra modul lehet nyitva"

#: ../build/src/preferences_gen.h:2360
msgid "this option toggles the behavior of shift clicking in darkroom mode"
msgstr ""
"ez a beállítás a shift+klikk viselkedését változtatja meg sötétkamra módban"

#: ../build/src/preferences_gen.h:2368
msgid "scroll to lighttable modules when expanded/collapsed"
msgstr "görgetés az átvilágító modulhoz kinyitáskor/becsukáskor"

#: ../build/src/preferences_gen.h:2380 ../build/src/preferences_gen.h:2400
msgid ""
"when this option is enabled then darktable will try to scroll the module to "
"the top of the visible list"
msgstr ""
"ha ez a beállítás engedélyezve van, a darktable megpróbálja a modult a "
"látható lista tetejére görgetni"

#: ../build/src/preferences_gen.h:2388
msgid "scroll to darkroom modules when expanded/collapsed"
msgstr "görgetés a sötétkamra modulhoz kinyitáskor/becsukáskor"

#: ../build/src/preferences_gen.h:2408
msgid "border around image in darkroom mode"
msgstr "keret a kép köré sötétkamra módban"

#: ../build/src/preferences_gen.h:2427
msgid ""
"process the image in darkroom mode with a small border. set to 0 if you "
"don't want any border."
msgstr ""
"sötétkamra módban a kép körül halvány keret jelenik meg. állítsd 0-ra, ha "
"nem akarsz keretet"

#: ../build/src/preferences_gen.h:2435
msgid "do high quality processing for slideshow"
msgstr "magas minőségű feldolgozás diavetítésnél"

#: ../build/src/preferences_gen.h:2447
msgid "same option as for export, but applies to slideshow."
msgstr "hasonló beállítás, mint exportálásnál, de a diavetítésre vonatkozik"

#: ../build/src/preferences_gen.h:2455
msgid "rating an image one star twice will not zero out the rating"
msgstr "egy képet kétszer értékelve egy csillagra ne nullázza le az értékelést"

#: ../build/src/preferences_gen.h:2467
msgid ""
"do not have the rating of one star behave as documented in the manual--an "
"image rated one star twice will result in a zero star rating."
msgstr ""
"az egycsillagos értékelés ne működjön úgy, ahogy a kézikönyvben olvasható – "
"egy kép egycsillagosra értékelése kétszer, nulla csillagos értékelést "
"eredményez"

#: ../build/src/preferences_gen.h:2475
msgid "method to use for getting the display profile"
msgstr "a kijelzőprofil lekérésének módja"

#: ../build/src/preferences_gen.h:2486 ../build/src/preferences_gen.h:2511
msgctxt "preferences"
msgid "all"
msgstr "mind"

#: ../build/src/preferences_gen.h:2491
msgctxt "preferences"
msgid "xatom"
msgstr "xatom"

#: ../build/src/preferences_gen.h:2496
msgctxt "preferences"
msgid "colord"
msgstr "colord"

#: ../build/src/preferences_gen.h:2514
msgid ""
"this option allows to force a specific means of getting the current display "
"profile. this is useful when one alternative gives wrong results"
msgstr ""
"a beállítás lehetővé teszi bizonyos kijelzőprofil lekérdezési eljárások "
"kényszerítését. ez akkor hasznos, ha valamelyik módszer rossz eredményt ad"

#: ../build/src/preferences_gen.h:2548
msgid "core options"
msgstr "általános beállítások"

#: ../build/src/preferences_gen.h:2551
msgid "memory in megabytes to use for thumbnail cache"
msgstr "bélyegkép gyorsítótár mérete megabájt"

#: ../build/src/preferences_gen.h:2571
msgid ""
"this controls how much memory is going to be used for thumbnails and other "
"buffers (needs a restart)."
msgstr ""
"azt szabályozza, hogy mennyi memóriát használhatnak a bélyegképek és egyéb "
"pufferek (újraindítást igényel)"

#: ../build/src/preferences_gen.h:2579
msgid "enable disk backend for thumbnail cache"
msgstr "bélyegkép gyorsítótár engedélyezése a lemezen"

#: ../build/src/preferences_gen.h:2591
msgid ""
"if enabled, write thumbnails to disk (.cache/darktable/) when evicted from "
"the memory cache. note that this can take a lot of memory (several gigabytes "
"for 20k images) and will never delete cached thumbnails again. it's safe "
"though to delete these manually, if you want. light table performance will "
"be increased greatly when browsing a lot. to generate all thumbnails of your "
"entire collection offline, run 'darktable-generate-cache'."
msgstr ""
"ha engedélyezve van, bélyegképeket ír a lemezre (.cache/darktable/), amikor "
"azok kikerülnek a memória gyorsítótárból. megjegyzendő, hogy ez sok "
"tárhelyet igényel (több gigabájtnyi 20 kilobájtos kép), amik sosem lesznek "
"törölve. ennek ellenére kézzel bármikor biztonságosan törölhetőek.  "
"böngészésnél az átvilágító nézet teljesítményét jelentős mértékben növeli. a "
"teljes gyűjteményhez offline módon is elkészíthető a bélyegkép gyorsítótár, "
"ehhez a \"darktable-generate-cache\" parancsot kell futtatni."

#: ../build/src/preferences_gen.h:2599
msgid "color manage cached thumbnails"
msgstr "színkezelt bélyegkép gyorsítótár"

#: ../build/src/preferences_gen.h:2611
msgid ""
"if enabled, cached thumbnails will be color managed so that lighttable and "
"filmstrip can show correct colors. otherwise the results may look wrong once "
"the display profile gets changed."
msgstr ""
"ha engedélyezve van, a gyorsítótárazott bélyegképek színkezeltek lesznek, "
"így az átvilágító és a filmszalag helyes színeket fog mutatni. egyébként az "
"eredmény hibásan nézhet ki, ha a kijelzőprofil megváltozik."

#: ../build/src/preferences_gen.h:2619
msgid "number of background threads"
msgstr "háttérfolyamatok száma"

#: ../build/src/preferences_gen.h:2638
msgid ""
"this controls for example how many threads are used to create thumbnails "
"during import. the cache will grow to a maximum of twice this number of full "
"resolution image buffers (needs a restart)."
msgstr ""
"szabályozza például, hogy importálásnál hány szálon fusson a bélyegképek "
"generálása. a gyorsítótár legfeljebb ennek a számnak és a teljes felbontású "
"kép puffer kétszeresére nő (újraindítást igényel)."

#: ../build/src/preferences_gen.h:2646
msgid "host memory limit (in MB) for tiling"
msgstr "kiszolgáló memóriakorlát (MB-ban) csempézésnél"

#: ../build/src/preferences_gen.h:2665
msgid ""
"this variable controls the maximum amount of memory (in MB) a module may use "
"during image processing. lower values will force memory hungry modules to "
"process image with increasing number of tiles. setting this to 0 will omit "
"any limit. values below 500 will be treated as 500 (needs a restart)."
msgstr ""
"ez a változó az egy modul által képfeldolgozáshoz használható "
"memóriakorlátot szabályozza megabájtban. a kisebb érték a memóriaéhesebb "
"modulokat arra kényszeríti, hogy egyre több csempéből dolgozzák fel a képet. "
"a 0 érték korlátlan memóriahasználatot engedélyez. az 500-nál kisebb érték "
"figyelmen kívül lesz hagyva. (újraindítást igényel)"

#: ../build/src/preferences_gen.h:2673
msgid "minimum amount of memory (in MB) for a single buffer in tiling"
msgstr "az egyes pufferek minimális memóriamérete (MB-ban) csempézésnél"

#: ../build/src/preferences_gen.h:2694
msgid ""
"if set to a positive, non-zero value this variable defines the minimum "
"amount of memory (in MB) that tiling should take for a single image buffer. "
"has precedence over heuristics based on host_memory_limit (needs a restart)."
msgstr ""
"pozitív, nullánál nagyobb értékre állítva meghatározza azt a minimális "
"memória mennyiséget (megabájtban), amit a csempézés el kell foglaljon egy "
"képpufferhez. elsőbbséget élvez a kiszolgáló memóriakorláttal szemben. "
"(újraindítást igényel)"

#: ../build/src/preferences_gen.h:2702
msgid "write sidecar file for each image"
msgstr "kísérőfájl mentése az egyes fájlokhoz"

#: ../build/src/preferences_gen.h:2714
msgid ""
"these redundant files can later be re-imported into a different database, "
"preserving your changes to the image."
msgstr ""
"ezek a redundáns fájlok később újraimportálhatóak egy másik adatbázisba, "
"megőrizve a kép összes változtatását."

#: ../build/src/preferences_gen.h:2722
msgid "store xmp tags in compressed format"
msgstr "xmp adatok tárolása tömörített formában"

#: ../build/src/preferences_gen.h:2743 ../build/src/preferences_gen.h:2758
msgctxt "preferences"
msgid "only large entries"
msgstr "csak a nagy elemeké"

#: ../build/src/preferences_gen.h:2761
msgid ""
"entries in xmp tags can get rather large and may exceed the available space "
"to store the history stack in output files. this option allows xmp tags to "
"be compressed and save space."
msgstr ""
"az xmp előzmény vermet tároló bejegyzései meglehetősen nagyra nőhetnek, "
"meghaladva a szabad lemezterületet. ez a beállítás lehetővé teszi az xmp "
"címkék tömörítését, ezzel tárhelyet megtakarítva."

#: ../build/src/preferences_gen.h:2769
msgid "omit hierarchy in simple tag lists"
msgstr "hierarchia kihagyása az egyszerű címkelistában"

#: ../build/src/preferences_gen.h:2781
msgid ""
"when exporting images the hierarchical tags are also added as a simple list "
"of non-hierarchical ones to make them visible to some other programs. when "
"this option is checked darktable will only include their last part and "
"ignore the rest. so 'foo|bar|baz' will only add 'baz'."
msgstr ""
"képek exportálásakor a hierarchikus címkék is belekerülnek egyszerű, nem "
"hierarchikus listaként, hogy láthatóak legyenek más programokban. ha ez az "
"opció engedélyezve van, a darktable csak az utolsó szintet teszi bele, és a "
"többit mellőri. például a „foo|bar|baz” egyszerűen „baz” lesz."

#: ../build/src/preferences_gen.h:2789
msgid "activate OpenCL support"
msgstr "OpenCL támogatás engedélyezése"

#: ../build/src/preferences_gen.h:2801
msgid ""
"if found, use OpenCL runtime on your system for improved processing speed. "
"can be switched on and off at any time."
msgstr ""
"OpenCL futtatókörnyezet használata a gyorsabb feldolgozási sebesség "
"érdekében, amennyiben az megtalálható a rendszeren. bármikor ki- és "
"bekapcsolható."

#: ../build/src/preferences_gen.h:2802 ../build/src/preferences_gen.h:2857
msgid "not available"
msgstr "nem érhető el"

#: ../build/src/preferences_gen.h:2805 ../build/src/preferences_gen.h:2809
#: ../build/src/preferences_gen.h:2860 ../build/src/preferences_gen.h:2864
msgid "not available on this system"
msgstr "nem elérhető ezen a rendszeren"

#: ../build/src/preferences_gen.h:2817
msgid "OpenCL scheduling profile"
msgstr "OpenCL ütemezési profil"

#. Adding the restore defaults button
#: ../build/src/preferences_gen.h:2828 ../build/src/preferences_gen.h:2853
#: ../src/gui/preferences.c:587
msgctxt "preferences"
msgid "default"
msgstr "alapértelmezett"

#: ../build/src/preferences_gen.h:2833
msgctxt "preferences"
msgid "multiple GPUs"
msgstr "több GPU"

#: ../build/src/preferences_gen.h:2838
msgctxt "preferences"
msgid "very fast GPU"
msgstr "nagyon gyors GPU"

#: ../build/src/preferences_gen.h:2856
msgid ""
"defines how preview and full pixelpipe tasks are scheduled on OpenCL enabled "
"systems. default - GPU processes full and CPU processes preview pipe "
"(adaptable by config parameters); multiple GPUs - process both pixelpipes in "
"parallel on two different GPUs; very fast GPU - process both pixelipes "
"sequentially on the GPU."
msgstr ""
"meghatározza az előnézet és a teljes pixelcsővezeték feladatok ütemezését az "
"OpenCL-t használó rendszereken. alapértelmezett- a GPU dolgozza fel a teljes "
"és a CPU az előnézet csövet (állítható a konfigurációs paraméterekben); több "
"GPU - mindkét pixelcsővezetéket párhuzamosan dolgozza fel két külön GPU; "
"nagyon gyors GPU - mindkét pixelcsővezetéket a GPU dolgozza fel egymás után."

#: ../build/src/preferences_gen.h:2872
msgid "always use LittleCMS 2 to apply output color profile"
msgstr "kimeneti színprofil készítése mindig LittleCMS 2-vel"

#: ../build/src/preferences_gen.h:2884
msgid "this is slower than the default."
msgstr "ez lassabb, mint az alapértelmezett."

#: ../build/src/preferences_gen.h:2892
msgid "do high quality resampling during export"
msgstr "magas minőségű újramintavételezés exportáláskor"

#: ../build/src/preferences_gen.h:2904
msgid ""
"the image will first be processed in full resolution, and downscaled at the "
"very end. this can result in better quality sometimes, but will always be "
"slower."
msgstr ""
"a kép először teljes felbontásban kerül feldolgozásra, és csak a legvégén "
"lesz lecsökkentve a mérete. ez bizonyos esetekben jobb minőséget eredményez, "
"de mindig lassabb lesz."

#: ../build/src/preferences_gen.h:2912
msgid "demosaicing for zoomed out darkroom mode"
msgstr "kicsinyített képek demosaic módja sötétkamra nézetben"

#: ../build/src/preferences_gen.h:2923
msgctxt "preferences"
msgid "always bilinear (fast)"
msgstr "mindig bilineáris (gyors)"

#: ../build/src/preferences_gen.h:2928 ../build/src/preferences_gen.h:2948
msgctxt "preferences"
msgid "at most PPG (reasonable)"
msgstr "legfeljebb PPG (elfogadható)"

#: ../build/src/preferences_gen.h:2933
msgctxt "preferences"
msgid "full (possibly slow)"
msgstr "teljes (lassú lehet)"

#: ../build/src/preferences_gen.h:2951
msgid ""
"interpolation when not viewing 1:1 in darkroom mode: bilinear is fastest, "
"but not as sharp. middle ground is using PPG + interpolation modes specified "
"below, full will use exactly the settings for full-size export. X-Trans "
"sensors use VNG rather than PPG as middle ground."
msgstr ""
"interpoláció, ha nem 1:1 arányban nézi sötétkamra módban. a bilineáris a "
"leggyorsabb, de nem éles. egy jó középút a PPG + a lentebbi interpolációs "
"módok. a teljes az exportálásnál is használt teljes méretű módszert jelenti. "
"X-Trans képérzékelők esetében PPG helyett VNG-t használ."

